<witd:WITD application='Work item type editor' version='1.0' xmlns:witd='http://schemas.microsoft.com/VisualStudio/2008/workitemtracking/typedef'>
  <WORKITEMTYPE name='Adoption Item'>
    <DESCRIPTION>general work item starter</DESCRIPTION>
    <FIELDS>
      <FIELD name='Sequence Value' refname='internalSequenceValue' type='smallString' dimension='reportable'>
        <HELPTEXT>From rtc 'Sequence Value'</HELPTEXT>
      </FIELD>
      <FIELD name='Resolved By' refname='resolver' type='contributor' dimension='reportable'>
        <HELPTEXT>From rtc 'Resolved By'</HELPTEXT>
      </FIELD>
      <FIELD name='Creation Date' refname='creationDate' type='timestamp' dimension='reportable'>
        <HELPTEXT>From rtc 'Creation Date'</HELPTEXT>
      </FIELD>
      <FIELD name='Start Date' refname='startDate' type='timestamp' dimension='reportable'>
        <HELPTEXT>From rtc 'Start Date'</HELPTEXT>
      </FIELD>
      <FIELD name='Due Date' refname='dueDate' type='timestamp' dimension='reportable'>
        <HELPTEXT>From rtc 'Due Date'</HELPTEXT>
      </FIELD>
      <FIELD name='Time Spent' refname='timeSpent' type='duration' dimension='reportable'>
        <HELPTEXT>From rtc 'Time Spent'</HELPTEXT>
      </FIELD>
      <FIELD name='Corrected Estimate' refname='correctedEstimate' type='duration' dimension='reportable'>
        <HELPTEXT>From rtc 'Corrected Estimate'</HELPTEXT>
      </FIELD>
      <FIELD name='Estimate' refname='duration' type='duration' dimension='reportable'>
        <HELPTEXT>From rtc 'Estimate'</HELPTEXT>
      </FIELD>
      <FIELD name='Approvals' refname='internalApprovals' type='approvals' dimension='reportable'>
        <HELPTEXT>From rtc 'Approvals'</HELPTEXT>
      </FIELD>
      <FIELD name='State Transitions' refname='internalStateTransitions' type='stateTransitions' dimension='reportable'>
        <HELPTEXT>From rtc 'State Transitions'</HELPTEXT>
      </FIELD>
      <FIELD name='Archived' refname='archived' type='boolean' dimension='reportable'>
        <HELPTEXT>From rtc 'Archived'</HELPTEXT>
      </FIELD>
      <FIELD name='Custom Attributes' refname='customAttributes' type='customAttribute' dimension='reportable'>
        <HELPTEXT>From rtc 'Custom Attributes'</HELPTEXT>
      </FIELD>
      <FIELD name='Summary' refname='summary' type='mediumHtml' dimension='reportable'>
        <HELPTEXT>From rtc 'Summary'</HELPTEXT>
      </FIELD>
      <FIELD name='Comments' refname='internalComments' type='comments' dimension='reportable'>
        <HELPTEXT>From rtc 'Comments'</HELPTEXT>
      </FIELD>
      <FIELD name='Approval Descriptors' refname='internalApprovalDescriptors' type='approvalDescriptors' dimension='reportable'>
        <HELPTEXT>From rtc 'Approval Descriptors'</HELPTEXT>
      </FIELD>
      <FIELD name='Resolution' refname='internalResolution' type='smallString' dimension='reportable'>
        <HELPTEXT>From rtc 'Resolution'</HELPTEXT>
      </FIELD>
      <FIELD name='Status' refname='internalState' type='smallString' dimension='reportable'>
        <HELPTEXT>From rtc 'Status'</HELPTEXT>
      </FIELD>
      <FIELD name='Subscribed By' refname='internalSubscriptions' type='subscriptions' dimension='reportable'>
        <HELPTEXT>From rtc 'Subscribed By'</HELPTEXT>
      </FIELD>
      <FIELD name='Resolution Date' refname='resolutionDate' type='timestamp' dimension='reportable'>
        <HELPTEXT>From rtc 'Resolution Date'</HELPTEXT>
      </FIELD>
      <FIELD name='Severity' refname='internalSeverity' type='severity' dimension='reportable'>
        <HELPTEXT>From rtc 'Severity'</HELPTEXT>
        <ALLOWEDVALUES expanditems='true'>
          <LISTITEM value='Unclassified' />
          <LISTITEM value='Minor' />
          <LISTITEM value='Normal' />
          <LISTITEM value='Major' />
          <LISTITEM value='Critical' />
          <LISTITEM value='Blocker' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='Description' refname='description' type='html' dimension='reportable'>
        <HELPTEXT>From rtc 'Description'</HELPTEXT>
      </FIELD>
      <FIELD name='Id' refname='id' type='integer' dimension='reportable'>
        <HELPTEXT>From rtc 'Id'</HELPTEXT>
      </FIELD>
      <FIELD name='Owned By' refname='owner' type='contributor' dimension='reportable'>
        <HELPTEXT>From rtc 'Owned By'</HELPTEXT>
      </FIELD>
      <FIELD name='Type' refname='workItemType' type='type' dimension='reportable'>
        <HELPTEXT>From rtc 'Type'</HELPTEXT>
      </FIELD>
      <FIELD name='Planned For' refname='target' type='interval' dimension='reportable'>
        <HELPTEXT>From rtc 'Planned For'</HELPTEXT>
      </FIELD>
      <FIELD name='Found In' refname='foundIn' type='deliverable' dimension='reportable'>
        <HELPTEXT>From rtc 'Found In'</HELPTEXT>
      </FIELD>
      <FIELD name='Filed Against' refname='category' type='category' dimension='reportable'>
        <HELPTEXT>From rtc 'Filed Against'</HELPTEXT>
      </FIELD>
      <FIELD name='Created By' refname='creator' type='contributor' dimension='reportable'>
        <HELPTEXT>From rtc 'Created By'</HELPTEXT>
      </FIELD>
      <FIELD name='Priority' refname='internalPriority' type='priority' dimension='reportable'>
        <HELPTEXT>From rtc 'Priority'</HELPTEXT>
        <ALLOWEDVALUES expanditems='true'>
          <LISTITEM value='Unassigned' />
          <LISTITEM value='Low' />
          <LISTITEM value='Medium' />
          <LISTITEM value='High' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='Tags' refname='internalTags' type='tags' dimension='reportable'>
        <HELPTEXT>From rtc 'Tags'</HELPTEXT>
      </FIELD>
      <FIELD name='Modified By' refname='modifiedBy' type='contributor' dimension='reportable'>
        <HELPTEXT>From rtc 'Modified By'</HELPTEXT>
      </FIELD>
      <FIELD name='Project Area' refname='projectArea' type='projectArea' dimension='reportable'>
        <HELPTEXT>From rtc 'Project Area'</HELPTEXT>
      </FIELD>
      <FIELD name='Restricted Access' refname='contextId' type='uuid' dimension='reportable'>
        <HELPTEXT>From rtc 'Restricted Access'</HELPTEXT>
      </FIELD>
      <FIELD name='Modified Date' refname='modified' type='timestamp' dimension='reportable'>
        <HELPTEXT>From rtc 'Modified Date'</HELPTEXT>
      </FIELD>
      <FIELD name='Impact' refname='com.ibm.team.rtc.attribute.impact' type='impact' dimension='reportable'>
        <HELPTEXT>From rtc 'Impact'</HELPTEXT>
        <ALLOWEDVALUES expanditems='true'>
          <LISTITEM value='Minor' />
          <LISTITEM value='Moderate' />
          <LISTITEM value='Significant' />
          <LISTITEM value='Major' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='Deploy Requirement(s)' refname='com.ibm.team.workitem.deployrequirementshtml' type='html' dimension='reportable'>
        <HELPTEXT>From rtc 'Deploy Requirement(s)'</HELPTEXT>
      </FIELD>
      <FIELD name='Affected Teams' refname='com.ibm.team.rtc.attribute.affectedTeams' type='teamAreaList' dimension='reportable'>
        <HELPTEXT>From rtc 'Affected Teams'</HELPTEXT>
      </FIELD>
      <FIELD name='Child Tasks' refname='com.tmb.team.workitem.att.child_tasks' type='enumerationList$com.ibm.team.workitem.enumeration.tasktype' dimension='reportable'>
        <HELPTEXT>From rtc 'Child Tasks'</HELPTEXT>
        <ALLOWEDVALUES expanditems='true'>
          <LISTITEM value='Unassigned' />
          <LISTITEM value='Test2 DBA Task' />
          <LISTITEM value='Test1 DBA Task' />
          <LISTITEM value='Development Task' />
          <LISTITEM value='Dev: Code Review' />
          <LISTITEM value='Dev: Design Review' />
          <LISTITEM value='Middleware Task' />
          <LISTITEM value='Production Validation Task' />
          <LISTITEM value='Test2 QA Task' />
          <LISTITEM value='QA Task' />
          <LISTITEM value='SCM Task' />
          <LISTITEM value='RBS Task' />
          <LISTITEM value='BA Task' />
          <LISTITEM value='Other Task' />
          <LISTITEM value='DBA Task' />
          <LISTITEM value='Staging QA Task' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='External ID' refname='com.ibm.team.workitem.externalid' type='smallString' dimension='reportable'>
        <HELPTEXT>From rtc 'External ID'</HELPTEXT>
      </FIELD>
      <FIELD name='Primary Component' refname='primary_component' type='mediumString' dimension='reportable'>
        <HELPTEXT>From rtc 'Primary Component'</HELPTEXT>
      </FIELD>
    </FIELDS>
  </WORKITEMTYPE>
</witd:WITD>