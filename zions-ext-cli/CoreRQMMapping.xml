<witmapping>

	<wit source="testplan"  target="Test Suite" translateUnmappedFields="false" notWorkItem="true">
	<!--  Nov 16th  
	 o Only r3 related test plans
	 o Only Active state.
	 o All RQM plans will go into a single test plan as suites in ADO.
	 -->
		<field source="nameHandler" target="name"/>
		<field source="descriptionHandler" target="description"/>
		<field source="planOwnerHandler" target="owner"/>
		<field source="stateHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
		<field source="planBlankHandler" target="area">
			<defaultvalue target="BaNCS\Testing\Releases\R3"/>
		</field>
		<field source="blankHandler" target="iteration">
			<defaultvalue target="BaNCS"/>		
		</field>
	</wit>
	<wit source="testsuite"  target="Test Suite" translateUnmappedFields="false" notWorkItem="true">
	<!--  Nov 16th  
	 o Only r3 related test plans
	 o Only Active state.
	 o All RQM plans will go into a single test plan as suites in ADO.
	 -->
		<field source="nameHandler" target="name"/>
		<field source="descriptionHandler" target="description"/>
		<field source="planOwnerHandler" target="owner"/>
		<field source="stateHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
		<field source="planBlankHandler" target="area">
			<defaultvalue target="BaNCS\Testing\Releases\R3"/>
		</field>
		<field source="blankHandler" target="iteration">
			<defaultvalue target="BaNCS"/>		
		</field>
	</wit>
	<wit source="testcase"  target="Test Case" translateUnmappedFields="false">
	<!-- 
	Nov 16
	o Category Test_Team will map to teamArea.  Mark will get mapping.
	 -->
		<field source="nameHandler" target="System.Title"/>
		<field source="testCasePriorityHandler" target="Microsoft.VSTS.Common.Priority">
			<defaultvalue target="4"/>
			<value source="0" target="1"/>
			<value source="1" target="2"/>
			<value source="2" target="3"/>
		</field>
		<field source="testCaseDescriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="customAttributesHandler" target="Custom.CustomAttributes"/>
		<field source="preconditionHandler" target="Custom.PreCondition"/>
		<field source="postconditionHandler" target="Custom.PostCondition"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="Design"/>
			<value source="Draft" target="Design"/>
			<value source="Ready for Peer Review" target="Ready for Peer"/>
			<value source="Ready for Business Review" target="Ready for Business"/>
			<value source="Final Approved" target="Approved"/>
			<value source="Retired" target="Closed"/>
		</field>
		<!-- testTeamHandler not created. -->
		<field source="coreTestCaseTeamAreaHandler" target="System.AreaPath">
			<defaultvalue target="BaNCS\Testing\Teams\Others"/>
			<value source="BaNCS Product: Test" target="BaNCS\Testing\Teams\Product"/>
			<value source="Business Unit: CDA" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Business Unit: ELO" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Business Unit: EML" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Business Unit: RLC" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Business Unit: Secondary" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Business Unit: UAT Triage" target="BaNCS\Testing\Teams\Business Units"/>
			<value source="Data Migration: Test" target="BaNCS\Testing\Teams\Data Migration"/>
			<value source="EIM: EDW Test" target="BaNCS\Testing\Teams\EIM"/>
			<value source="Finance: EDW" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Evolv" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Evolv - TI" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: FAH" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Financial Accounting Hub" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Frontier" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Loan Operations" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: ODS" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: R3 BaNCS Finance" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Finance: Recon" target="BaNCS\Testing\Teams\Finance"/>
			<value source="Interfaces: UAT" target="BaNCS\Testing\Teams\Interface"/>
			<value source="Interfaces: BaNCS Test" target="BaNCS\Testing\Teams\Interface"/>
			<value source="Interfaces: Tier 2" target="BaNCS\Testing\Teams\Interface"/>
			<value source="N/A" target="BaNCS\Testing\Teams\NoTeamAssignment"/>
			<value source="NFR: Test" target="BaNCS\Testing\Teams\NFR"/>
			<value source="ODS Data" target="BaNCS\Testing\Teams\ODS Data"/>
			<value source="R2R" target="BaNCS\Testing\Teams\R2R"/>
			<value source="Reports: BaNCS Test" target="BaNCS\Testing\Teams\Reports"/>
			<value source="Reports: ODS Test" target="BaNCS\Testing\Teams\Reports"/>
			<value source="Rollout" target="BaNCS\Testing\Teams\Rollout"/>
			<value source="SNF: Test" target="BaNCS\Testing\Teams\SNF"/>
		</field>
		<field source="blankHandler" target="System.IterationPath">
			<defaultvalue target="BaNCS"/>		
		</field>
		<field source="stepsHandler" target="Microsoft.VSTS.TCM.Steps"/>
		<excluded>
			<field name="Execution_Count"/>
			<field name="Risk_Model__RE__Calculation"/>
			<field name="Test_Case_Risk"/>
			<field name="Unresolved_Defect_Count"/>
			<field name="Test_Case_Important_Level"/>
			<field name="Active"/>
			<field name="Environment"/>
			<field name="UAT_Groups"/>
			<field name="Vendor_Priority"/>
			<field name="Workstream"/>
			<field name="Business_Approval"/>
			<field name="Execution_Planning"/>
			<field name="Automated_TC_Name"/>
			<field name="_k5J38USxEeiuULVYIY76YA"/>
			<field name="_k5l80ESxEeiuULVYIY76YA"/>
			<field name="_k4meUUSxEeiuULVYIY76YA"/>
			<field name="Vendor_TC_Name"/>
			<field name="Test_Team"/>
		</excluded>
	</wit>
	<wit source="executionresult"  target="Result" translateUnmappedFields="false">
		<field source="resultIdHandler" target="id"/>
		<field source="resultStartDateHandler" target="startedDate"/>
		<field source="resultTestcaseReferenceHandler" target="testCaseReferenceId"/>
		<field source="blankHandler" target="priority">
			<defaultvalue target="2"/>		
		</field>
		
		<field source="resultStateHandler" target="outcome">
			<defaultvalue target="Unspecified"/>		
			<value source="com.ibm.rqm.execution.common.state.passed" target="Passed"/>
			<value source="com.ibm.rqm.execution.common.state.paused" target="Paused"/> 
			<value source="com.ibm.rqm.execution.common.state.inprogress" target="InProgress"/> 
			<value source="com.ibm.rqm.execution.common.state.notrun" target="NotExecuted"/> 
			<value source="com.ibm.rqm.execution.common.state.perm_failed" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.incomplete" target="Paused"/> 
			<value source="com.ibm.rqm.execution.common.state.inconclusive" target="Inconclusive"/> 
			<value source="com.ibm.rqm.execution.common.state.part_blocked" target="Blocked"/> 
			<value source="com.ibm.rqm.execution.common.state.deferred" target="NotExecuted"/> 
			<value source="com.ibm.rqm.execution.common.state.failed" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.error" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.blocked" target="Blocked"/> 		
			</field>
		<field source="resultStateHandler" target="state">
			<defaultvalue target="Completed"/>		
			<!--  <value source="com.ibm.rqm.execution.common.state.passed" target="Completed"/>
			<value source="com.ibm.rqm.execution.common.state.perm_failed" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.deferred" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.failed" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.error" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.blocked" target="Completed"/> 	-->	
		</field>
		<!-- <field source="resultOwnerHandler" target="owner"/> -->
		
	</wit>
	<wit source="configuration"  target="Configuration" translateUnmappedFields="false">
		<field source="nameHandler" target="name"/>
		<field source="summaryHandler" target="description"/>
		<field source="blankHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
	</wit>
	<wit source="testplan"  target="Test Suite WI" translateUnmappedFields="false">
		<field source="customAttributesHandler" target="Custom.CustomAttributes"/>
	</wit> 
	<wit source="testsuite"  target="Test Suite WI" translateUnmappedFields="false">
		<field source="customAttributesHandler" target="Custom.CustomAttributes"/>
	</wit> 

	
</witmapping>