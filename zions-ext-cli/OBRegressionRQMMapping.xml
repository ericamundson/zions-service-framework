<witmapping>
	<wit source="testplan"  target="Test Plan" translateUnmappedFields="false" notWorkItem="true">
		<field source="nameHandler" target="name"/>
		<field source="descriptionHandler" target="description"/>
		<field source="planOwnerHandler" target="owner"/>
		<field source="stateHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
		<field source="planBlankHandler" target="area">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
	<wit source="testsuite"  target="Test Suite" translateUnmappedFields="false" notWorkItem="true">
		<field source="nameHandler" target="name"/>
		<field source="descriptionHandler" target="description"/>
		<field source="planOwnerHandler" target="owner"/>
		<field source="stateHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
		<field source="planBlankHandler" target="area">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
	<wit source="testcase"  target="Test Case" translateUnmappedFields="false">
		<field source="nameHandler" target="System.Title"/>
		<field source="testCasePriorityHandler" target="Microsoft.VSTS.Common.Priority">
			<defaultvalue target="2"/>
			<value source="Low" target="1"/>
			<value source="Medium" target="2"/>
			<value source="High" target="3"/>
		</field>
		<field source="customAttributesHandler" target="Custom.CustomAttributes"/>
		<field source="testCaseDescriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="Design"/>
		</field>
		<field source="oBTestCaseTeamAreaHandler" target="System.AreaPath">
			<defaultvalue target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression"/>
			<value source="OB- BET" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-BET"/>
			<value source="OB-Accounts" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Accounts"/>
			<value source="OB-Admin tab" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Admin Tab"/>
			<value source="OB-CCOnline" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-CCOnline"/>
			<value source="OB-COBRA" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-COBRA"/>
			<value source="OB-External Transfers" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-External Transfers"/>
			<value source="OB-FAST" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-FAST"/>
			<value source="OB-IB Admin" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-IB Admin"/>
			<value source="OB-Internal Transfers" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Internal Transfers"/>
			<value source="OB-My Bank" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-My Bank"/>
			<value source="OB-Payments" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Payments"/>
			<value source="OB-SAM" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-SAM"/>
			<value source="OB-Services tab" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Services tab"/>
			<value source="OB-SET" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-SET"/>
			<value source="OB-VIP Exceptions" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-VIP Exceptions"/>
			<value source="OB-Wires eSignature" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Wires eSignature"/>
			<value source="OB-Workflow App" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-Workflow App"/>
			<value source="ZAP" target="DigitalBanking\OB\QA Regression\ACI Online Banking 3.7 Regression\OB-ZAP"/>
		</field>
		<field source="blankHandler" target="System.IterationPath">
			<defaultvalue target="DigitalBanking"/>		
		</field>
		<field source="stepsHandler" target="Microsoft.VSTS.TCM.Steps"/>
		<excluded>
			<field name="Execution_Count"/>
			<field name="Risk_Model__RE__Calculation"/>
			<field name="Test_Case_Risk"/>
			<field name="Unresolved_Defect_Count"/>
			<field name="Test_Case_Important_Level"/>
		</excluded>
	</wit>
	<wit source="executionresult"  target="Result" translateUnmappedFields="false">
		<field source="resultIdHandler" target="id"/>
		<field source="resultStartDateHandler" target="startedDate"/>
		<field source="resultTestcaseTitleHandler" target="testCaseTitle"/>
		<field source="blankHandler" target="priority">
			<defaultvalue target="2"/>		
		</field>
		
		<field source="resultStateHandler" target="outcome">
			<defaultvalue target="Active"/>		
			<value source="com.ibm.rqm.execution.common.state.passed" target="Passed"/>
			<value source="com.ibm.rqm.execution.common.state.paused" target="Active"/> 
			<value source="com.ibm.rqm.execution.common.state.inprogress" target="Active"/> 
			<value source="com.ibm.rqm.execution.common.state.notrun" target="Active"/> 
			<value source="com.ibm.rqm.execution.common.state.perm_failed" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.incomplete" target="Active"/> 
			<value source="com.ibm.rqm.execution.common.state.inconclusive" target="Active"/> 
			<value source="com.ibm.rqm.execution.common.state.part_blocked" target="Block"/> 
			<value source="com.ibm.rqm.execution.common.state.deferred" target="Not applicable"/> 
			<value source="com.ibm.rqm.execution.common.state.failed" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.error" target="Failed"/> 
			<value source="com.ibm.rqm.execution.common.state.blocked" target="Block"/> 		
			</field>
		<field source="resultStateHandler" target="state">
			<defaultvalue target="Pending"/>		
			<value source="com.ibm.rqm.execution.common.state.passed" target="Completed"/>
			<value source="com.ibm.rqm.execution.common.state.perm_failed" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.deferred" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.failed" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.error" target="Completed"/> 
			<value source="com.ibm.rqm.execution.common.state.blocked" target="Completed"/> 		
		</field>
		<!-- <field source="resultOwnerHandler" target="owner"/> -->
		
	</wit>
	<wit source="configuration"  target="Configuration" translateUnmappedFields="false">
		<field source="nameHandler" target="name"/>
		<field source="summaryHandler" target="description"/>
		<field source="blankHandler" target="state">
			<defaultvalue target="Active"/>
		</field>
	</wit>
	<!-- 
	<wit source="testplan"  target="Test Plan WI" translateUnmappedFields="false">
		<field source="customAttributesHandler" target="Custom.CustomAttributes"/>
	</wit> -->
	
</witmapping>