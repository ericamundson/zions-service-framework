          <LISTITEM value='Unassigned' />
          <LISTITEM value='P4 - Could Have' />
          <LISTITEM value='P3 - Should Have' />
          <LISTITEM value='P2 - Must Have' />
          <LISTITEM value='P1 - Show Stopper' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='Tags' refname='internalTags' type='tags' dimension='reportable'>
        <HELPTEXT>From rtc 'Tags'</HELPTEXT>
      </FIELD>
