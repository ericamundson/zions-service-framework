<witd:WITD application='Work item type editor' version='1.0' xmlns:witd='http://schemas.microsoft.com/VisualStudio/2008/workitemtracking/typedef'>
  <WORKITEMTYPE name='Epic'>
    <DESCRIPTION>general work item starter</DESCRIPTION>
    <FIELDS>
      <FIELD name='Severity' refname='internalSeverity' type='severity' dimension='reportable'>
        <HELPTEXT>From rtc 'Severity'</HELPTEXT>
        <ALLOWEDVALUES expanditems='true'>
          <LISTITEM value='Unassigned' />
          <LISTITEM value='S5 - Trivial - Cosmetic, Color, Spelling Errors' />
          <LISTITEM value='S4 - Minor - Easy Workaround' />
          <LISTITEM value='S3 - Major - Acceptable Workaround' />
          <LISTITEM value='S2 - Critical - None or Unacceptable Workaround, Major Feature Failure' />
          <LISTITEM value='S1 - Blocker - Application Crashes, Loss of Data, or Data Corruption' />
        </ALLOWEDVALUES>
      </FIELD>
      <FIELD name='Summary' refname='summary' type='mediumHtml' dimension='reportable'>
        <HELPTEXT>From rtc 'Summary'</HELPTEXT>
      </FIELD>
      <FIELD name='Resolution' refname='internalResolution' type='smallString' dimension='reportable'>
        <HELPTEXT>From rtc 'Resolution'</HELPTEXT>
      </FIELD>
