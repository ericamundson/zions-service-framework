<service>
      <id>zions-queue-pubsub-wit-microservice</id>
      <name>zions-queue-pubsub-wit-microservice</name>
      <description>This service runs zions-queue-pubsub-wit-microservice.</description>
      <executable>java</executable>
      
      <arguments>-Xrs -Xmx256m -jar "{{deploy.path}}/zions-queue-pubsub-wit-microservice.jar" --queue.name=WIType-queue  --ado.topics=workitem.updated --tfs.url={{tfs.url}} --logging.file="{{deploy.path}}/zions-queue-pubsub-wit-microservice.log" </arguments>
	  
      <logmode>rotate</logmode>
      <logpath>{{deploy.path}}\wrapper-logs\zions-queue-pubsub-wit-microservice</logpath>
</service>