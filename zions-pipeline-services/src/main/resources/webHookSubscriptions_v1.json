{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "type": "webHookSubscriptions",
            "context": "eto-dev",
            "projects": "ReleaseEngineering",
            "eventTypes": "git.push",
            "consumerUrl": "https://releaseengineeringprovisioner-zionsbancorporation.msappproxy.net",
            "publisherInputs": {
                "repository": "",
                "branch": "",
                "pushedBy": "",
                "projectId": ""
            }
        }
    ],
    "required": [
        "type",
        "projects",
        "eventTypes",
        "consumerUrl",
        "publisherInputs"
    ],
    "properties": {
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "webHookSubscriptions"
            ]
        },
        "context": {
            "$id": "#/properties/context",
            "type": "string",
            "title": "The context schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "eto-dev"
            ]
        },
        "projects": {
            "$id": "#/properties/projects",
            "type": "string",
            "title": "The projects schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "ReleaseEngineering"
            ]
        },
        "eventTypes": {
            "$id": "#/properties/eventTypes",
            "type": "string",
            "title": "The eventTypes schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "git.push"
            ]
        },
        "consumerUrl": {
            "$id": "#/properties/consumerUrl",
            "type": "string",
            "title": "The consumerUrl schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://releaseengineeringprovisioner-zionsbancorporation.msappproxy.net"
            ]
        },
        "consumerUserName": {
            "$id": "#/properties/consumerUserName",
            "type": "string",
            "title": "The consumerUserName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://releaseengineeringprovisioner-zionsbancorporation.msappproxy.net"
            ]
        },
        "consumerPassword": {
            "$id": "#/properties/consumerPassword",
            "type": "string",
            "title": "The consumerPassword schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://releaseengineeringprovisioner-zionsbancorporation.msappproxy.net"
            ]
        },
        "vault": {
            "$id": "#/properties/vault",
            "type": "object",
            "title": "The vault schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "engine": "secret",
                    "path": "zions-service-framework"
                }
            ],
            "required": [
                "engine",
                "paths"
            ],
            "properties": {
                "engine": {
                    "$id": "#/properties/repository/properties/engine",
                    "type": "string",
                    "title": "The engine schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "secret"
                    ]
                },
                "paths": {
                  "$id": "#root/vault/paths", 
                  "title": "Paths", 
                  "type": "array",
                  "default": [],
                  "items":{
                    "$id": "#root/vault/paths/items", 
                    "title": "Items", 
                    "type": "string",
                    "default": "",
                    "examples": [
                      "zions-service-framework"
                    ],
                    "pattern": "^.*$"
                  }
                }
            },
            "additionalProperties": false
        },
        "publisherInputs": {
            "$id": "#/properties/publisherInputs",
            "type": "object",
            "title": "The publisherInputs schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "repository": "",
                    "branch": "",
                    "pushedBy": "",
                    "projectId": ""
                }
            ],
			"required": [],
			"patternProperties": {
				"^.*$": {
					"anyOf": [
						{
							"type": "string"
						},
						{
							"type": "boolean"
						},
						{
							"type": "integer"
						},
						{
							"type": "null"
						}
					]
				}
			},
			"additionalProperties": false
        }
    },
    "additionalProperties": false
}