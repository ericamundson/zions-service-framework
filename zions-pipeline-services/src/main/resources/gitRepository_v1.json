{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "name": "TestRepo",
            "type": "gitRepository",
            "project": "ALMOpsTest",
            "branches": [
                {
                    "name": "feature/support",
                    "baseName": "master"
                }
            ]
        }
    ],
    "required": [
        "name",
        "type"
    ],
    "properties": {
        "isLast": {
            "$id": "#/properties/isLast",
            "type": "boolean",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "true"
            ]
        },        
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "TestRepo"
            ]
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "gitRepository"
            ]
        },
        "project": {
            "$id": "#/properties/project",
            "type": "string",
            "title": "The project schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "ALMOpsTest"
            ]
        },
        "context": {
            "$id": "#/properties/context",
            "type": "string",
            "title": "The context schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "ALMOpsTest"
            ]
        },
        "branches": {
            "$id": "#/properties/branches",
            "type": "array",
            "title": "The branches schema",
            "description": "An explanation about the purpose of this instance.",
            "default": [],
            "examples": [
                [
                    {
                        "name": "feature/support",
                        "baseName": "master"
                    }
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/branches/items",
                "anyOf": [
                    {
                        "$id": "#/properties/branches/items/anyOf/0",
                        "type": "object",
                        "title": "The first anyOf schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": {},
                        "examples": [
                            {
                                "name": "feature/support",
                                "baseName": "master"
                            }
                        ],
                        "required": [
                            "name",
                            "baseName"
                        ],
                        "properties": {
                            "name": {
                                "$id": "#/properties/branches/items/anyOf/0/properties/name",
                                "type": "string",
                                "title": "The name schema",
                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "feature/support"
                                ]
                            },
                            "baseName": {
                                "$id": "#/properties/branches/items/anyOf/0/properties/baseName",
                                "type": "string",
                                "title": "The baseName schema",
                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "master"
                                ]
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        }
    },
    "additionalProperties": false
}