{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1604951643.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"type",
		"yamlFile"
	],
	"properties": {
		"type": {
			"$id": "#root/type", 
			"title": "Type", 
			"type": "string",
			"default": "",
			"examples": [
				"runXLDeployApply"
			],
			"pattern": "^.*$"
		},
		"yamlFile": {
			"$id": "#root/yamlFile", 
			"title": "Yamlfile", 
			"type": "string",
			"default": "",
			"examples": [
				".pipeline/xl-deploy.yaml"
			],
			"pattern": "^.*$"
		},
		"vault": {
			"$id": "#root/vault", 
			"title": "Vault", 
			"type": "object",
			"required": [
				"engine",
				"paths"
			],
			"properties": {
				"engine": {
					"$id": "#root/vault/engine", 
					"title": "Engine", 
					"type": "string",
					"default": "",
					"examples": [
						"secrets"
					],
					"pattern": "^.*$"
				},
				"paths": {
					"$id": "#root/vault/paths", 
					"title": "Paths", 
					"type": "array",
					"default": [],
					"items":{
						"$id": "#root/vault/paths/items", 
						"title": "Items", 
						"type": "string",
						"default": "",
						"examples": [
							"zions-service-framework"
						],
						"pattern": "^.*$"
					}
				}
			}
		}
,
		"values": {
			"$id": "#root/values", 
			"title": "Values", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/values/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"name",
					"value"
				],
				"properties": {
					"name": {
						"$id": "#root/values/items/name", 
						"title": "Name", 
						"type": "string",
						"default": "",
						"examples": [
							"test1"
						],
						"pattern": "^.*$"
					},
					"value": {
						"$id": "#root/values/items/value", 
						"title": "Value", 
						"type": "string",
						"default": "",
						"examples": [
							"avalue1"
						],
						"pattern": "^.*$"
					}
				}
			}

		}
	}
}
