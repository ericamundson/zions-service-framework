<service>
      <id>zions-vsts-mapfield-microservice0</id>
      <name>zions-vsts-mapfield-microservice0</name>
      <description>This service runs zions-vsts-mapfield-microservice.</description>
      <executable>java</executable>
      
      <arguments>-Xrs -Xmx256m -Dspring.profiles.active={{spring.profile}} -jar "{{deploy.path}}/zions-vsts-mapfield-microservice.jar" --queue.name=WorkItem-mapfield-queue --ado.topics=workaround --tfs.collection={{tfs.collection}} --tfs.url=https://dev.azure.com --logging.file="{{deploy.path}}/zions-vsts-mapfield-microservice0.log" </arguments>
 	  
      <logmode>rotate</logmode>
      <logpath>{{deploy.path}}\wrapper-logs\zions-vsts-mapfield-microservice0</logpath>
</service>