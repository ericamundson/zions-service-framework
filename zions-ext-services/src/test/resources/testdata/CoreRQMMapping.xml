<witmapping>
	<wit source="testplan"  target="TestPlan" translateUnmappedFields="false">
		<field source="nameHandler" target="System.Title"/>
		<field source="descriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="Active"/>
			<value source='Closed' target="Inactive"/>
		</field>
		<field source="blankHandler" target="System.AreaPath">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="System.Iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
	<wit source="testplan"  target="TestSuite" translateUnmappedFields="false">
		<field source="nameHandler" target="System.Title"/>
		<field source="descriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="In Planning"/>
			<value source='Approved' target="In Progress"/>
			<value source='Retired' target="Closed"/>
		</field>
		<field source="blankHandler" target="System.AreaPath">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="System.Iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
	<wit source="testsuite"  target="TestSuite" translateUnmappedFields="false">
		<field source="nameHandler" target="System.Title"/>
		<field source="descriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="In Planning"/>
			<value source='Approved' target="In Progress"/>
			<value source='Retired' target="Closed"/>
		</field>
		<field source="blankHandler" target="System.AreaPath">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="System.Iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
	<wit source="testcase"  target="TestCase" translateUnmappedFields="false">
		<field source="testCaseNameHandler" target="System.Title"/>
		<field source="testCaseDescriptionHandler" target="System.Description"/>
		<field source="ownerHandler" target="System.AssignedTo"/>
		<field source="stateHandler" target="System.State">
			<defaultvalue target="Design"/>
		</field>
		<field source="blankHandler" target="System.AreaPath">
			<defaultvalue target="DigitalBanking\OB"/>
		</field>
		<field source="blankHandler" target="System.Iteration">
			<defaultvalue target="DigitalBanking"/>		
		</field>
	</wit>
</witmapping>