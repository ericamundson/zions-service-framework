r\n        <text:richTextBody>\r\n          <div>\r\n            <h:div xmlns:h=\"http://www.w3.org/1999/xhtml\">\r\n              <h:p style=\"margin-left: 0px;\" id=\"_1585\" dir=\"ltr\">\u00c2</h:p>\r\n              <h:p style=\"margin-left: 0px;\" id=\"_1586\" dir=\"ltr\">\r\n                <h:span style=\"font-family: arial,helvetica,sans-serif; font-size: 11pt;\">{Sub Topic:Interest Accumulators}</h:span>\r\n              </h:p>\r\n              <h:ul dir=\"ltr\">\r\n                <h:li style=\"text-align: justify; margin-left: 80px;\" id=\"_1588\">System should have a field for both YTD Interest and LYTD Interest.</h:li>\r\n                <h:li style=\"text-align: justify; margin-left: 40px;\" id=\"_1594\">\r\n                  <h:span style=\"font-family: arial,helvetica,sans-serif; font-size: 11pt;\">The YTD Interest Amount is updated whenever capitalization takes place whether in batch or online using forced interest capitalization transactions.</h:span>\r\n                </h:li>\r\n                <h:li style=\"text-align: justify; margin-left: 40px;\" id=\"_1595\">\r\n                  <h:span style=\"font-family: arial,helvetica,sans-serif; font-size: 11pt;\">The LYTD Interest Amount is overwritten with the YTD Interest Amount at the year-end (December 31st)</h:span>\r\n                </h:li>\r\n                <h:li style=\"text-align: justify; margin-left: 40px;\" id=\"_1596\">\r\n                  <h:span style=\"font-family: arial,helvetica,sans-serif; font-size: 11pt;\">The YTD Interest Amount is made zero at year-end after moving the amount to LYTD Interest</h:span>\r\n                </h:li>\r\n              </h:ul>\r\n              <h:p style=\"margin-left: 0px;\" id=\"_1589\" dir=\"ltr\">\u00c2</h:p>\r\n            </h:div>\r\n          </div>\r\n        </text:richTextBody>\r\n      </text:text>\r\n    </ds:content>\r\n    <ds:aggregatedContent/>\r\n    <ds:moduleContext>\r\n      <rrm:contextBinding>\r\n        <rrm:about>https://clm.cs.zionsbank.com/rm/resources/_ded471c4cdf542359ab2eb108eee0aa8</rrm:about>\r\n        <rrm:identifier>26880</rrm:identifier>\r\n        <rrm:title>\u00c2</rrm:title>\r\n        <rrm:moduleDisplayName>26728: FS-TD-010 - Notices and Inquiry</rrm:moduleDisplayName>\r\n        <rrm:moduleUrl>https://clm.cs.zionsbank.com/rm/resources/_frOPQFDzEeWblrEplHqOHQ</rrm:moduleUrl>\r\n        <rrm:parentBindingUrl>https://clm.cs.zionsbank.com/rm/resources/_3c9f2014aa9b4776b0a225389d4127c1</rrm:parentBindingUrl>\r\n        <rrm:section>8.1.0-1</rrm:section>\r\n        <rrm:bookOrder>190</rrm:bookOrder>\r\n        <rrm:depth>3</rrm:depth>\r\n        <rrm:isHeading>false</rrm:isHeading>\r\n      </rrm:contextBinding>\r\n    </ds:moduleContext>\r\n    <ds:traceability>\r\n      <ds:links>\r\n        <ds:Link type=\"Link\">\r\n          <rrm:title>Satisfied By</rrm:title>\r\n          <ds:linkType>https://clm.cs.zionsbank.com/rm/types/_m3VFsRNGEeSmasotILOx6w</ds:linkType>\r\n          <ds:isParentLink>false</ds:isParentLink>\r\n          <ds:isChildLink>false</ds:isChildLink>\r\n          <rrm:relation>https://clm.cs.zionsbank.com/rm/resources/_JoG0sGhdEeW1TKZDoSad_Q</rrm:relation>\r\n          <rrm:identifier>42089</rrm:identifier>\r\n          <ds:content>\r\n            <rrm:title>YTD INTEREST</rrm:title>\r\n            <rrm:identifier>42089</rrm:identifier>\r\n            <rrm:description/>\r\n            <rrm:format>Text</rrm:format>\r\n            <ds:artifactFormat>TCS Data Element</ds:artifactFormat>\r\n          </ds:content>\r\n        </ds:Link>\r\n        <ds:Link type=\"Link\">\r\n          <rrm:title>Satisfied By</rrm:title>\r\n          <ds:linkType>https://clm.cs.zionsbank.com/rm/types/_m3VFsRNGEeSmasotILOx6w</ds:linkType>\r\n          <ds:isParentLink>false</ds:isParentLink>\r\n          <ds:isChildLink>false</ds:isChildLink>\r\n          <rrm:relation>https://clm.cs.zionsbank.com/rm/resources/_lAZywIQNEeW2Y-5MvJERJA</rrm:relation>\r\n          <rrm:identifier>57094</r