<service>
      <id>zions-vsts-populate-child-microservice</id>
      <name>zions-vsts-populate-child-microservice</name>
      <description>This service runs zions-vsts-populate-child-microservice.</description>
      <executable>java</executable>
      
      <arguments>-Xrs -Xmx256m -jar "{{deploy.path}}/zions-vsts-populate-child-microservice.jar" --queue.name=WorkItem-populatechild-queue  --ado.topics="Component,Sub Component,Epic,Feature,User Story" --tfs.url={{tfs.url}} --logging.file="{{deploy.path}}/zions-vsts-populate-child-microservice.log" </arguments>
	  
      <logmode>rotate</logmode>
      <logpath>{{deploy.path}}\wrapper-logs\zions-vsts-populate-child-microservice</logpath>
</service>