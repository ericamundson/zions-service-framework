          <LISTITEM value='Unassigned' />
          <LISTITEM value='S5 - Trivial - Cosmetic, Color, Spelling Errors' />
          <LISTITEM value='S4 - Minor - Easy Workaround' />
          <LISTITEM value='S3 - Major - Acceptable Workaround' />
          <LISTITEM value='S2 - Critical - None or Unacceptable Workaround, Major Feature Failure' />
