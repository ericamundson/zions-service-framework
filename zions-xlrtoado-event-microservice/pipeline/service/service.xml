<service>
      <id>zions-xlrtoado-event-microservice</id>
      <name>zions-xlrtoado-event-microservice</name>
      <description>This service runs zions-xlrtoado-event-microservice.</description>
      <executable>java</executable>
      
      <arguments>-Xrs -Xmx256m -jar -Dspring.profiles.active={{spring.profile}} "{{deploy.path}}/zions-xlrtoado-event-microservice.jar" --queue.name=XLRToADO-queue  --ado.topics=none --tfs.url={{tfs.url}} --logging.file="{{deploy.path}}/zions-xlrtoado-event-microservice.log" </arguments>
	  
      <logmode>rotate</logmode>
      <logpath>{{deploy.path}}\wrapper-logs\zions-xlrtoado-event-microservice</logpath>
</service>