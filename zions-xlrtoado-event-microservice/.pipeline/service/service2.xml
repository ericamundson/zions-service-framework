<service>
      <id>zions-xlrtoado-event-microservice2</id>
      <name>zions-xlrtoado-event-microservice2</name>
      <description>This service runs zions-xlrtoado-event-microservice2.</description>
      <executable>java</executable>
      
      <arguments>-Xrs -Xmx256m -jar -Dspring.profiles.active={{spring.profile}} "{{deploy.path}}/zions-xlrtoado-event-microservice.jar" --tfs.url={{tfs.url}} --logging.file="{{deploy.path}}/zions-xlrtoado-event-microservice2.log" </arguments>
	  
      <logmode>rotate</logmode>
      <logpath>{{deploy.path}}\wrapper-logs\zions-xlrtoado-event-microservice2</logpath>
</service>